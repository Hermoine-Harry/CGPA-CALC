<%- include("partials/header") -%>


<div class="container">
  <div class="back-btn-div">
      <a href="/index"><i class="fas fa-arrow-circle-left"></i></a>
      <h1 class="brand"><span>CGPA</span> Calculator</h1>
  </div>
  <div class="wrapper">
    <div class="dept-info">
      <div class="logo-div">
        <img src="/css/mit1-logo.png" alt="" class="logo">
      </div>
      <div class="logo-title">
        <ul>
          <li>Madras Institute of Technology campus,</li>
          <li>Anna University,</li>
          <li>Chromepet.</li>
        </ul>
      </div>
    </div>
    <div class="selected-info">
      <form >
        <p>
          <label>Department</label>
          <input type="text" name="" value="AERONAUTICAL" readonly>
        </p>
        <p>
          <label>Regulation</label>
          <input type="text" name="" value="R2019" readonly>
        </p>
        <p>
          <label>Semester</label>
          <input type="text" name="" value="VIII" readonly>
        </p>
        <p>
          <label>Total Credits</label>
          <input type="text" id="cr" name="" value="14" readonly>
        </p>
        <p>
          <label>NO Of Subjects</label>
          <input type="number" name="" id="nos" value="3" readonly>
        </p>
      </form>
    </div>


  </div>
</div>



<!-- table -->
